    "react-toastify": "^10.0.5",
    "reading-time": "^1.5.0",
    "remark-gfm": "^4.0.0",
    "remark-unwrap-images": "^4.0.0",
    "server-only": "^0.0.1",
    "sharp": "^0.33.4"
  },
